{
    "python.analysis.typeCheckingMode": "off",
    "yaml.schemas": {
        "https://json.schemastore.org/github-issue-config.json": "./.github/ISSUE_TEMPLATE/config.yml"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.css": true,
        "**/.css.map": true,
        "vanilla_installer/__pycache__": true
    },
    "hide-files.files": [
        "vanilla_installer/__pycache__"
    ],
    "cSpell.words": [
        "authdata",
        "modpack",
        "nobackslash"
    ]// ,
